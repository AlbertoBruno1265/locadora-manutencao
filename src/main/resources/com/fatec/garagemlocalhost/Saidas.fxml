<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fatec.garagemlocalhost.SaidasController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TableView fx:id="tabelaSaida" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="colunaID" prefWidth="75.0" text="ID PEDIDO" />
          <TableColumn fx:id="colunaPlaca" prefWidth="75.0" text="PLACA" />
            <TableColumn fx:id="colunaModelo" prefWidth="75.0" text="MODELO" />
            <TableColumn fx:id="colunaCor" prefWidth="75.0" text="COR" />
            <TableColumn fx:id="colunaAno" prefWidth="75.0" text="ANO" />
            <TableColumn fx:id="colunaInstante" prefWidth="75.0" text="INSTANTE" />
            <TableColumn fx:id="colunaStatus" prefWidth="75.0" text="STATUS" />
            <TableColumn fx:id="colunaAtualizar" prefWidth="75.0" text="ATUALIZAR" />
        </columns>
      </TableView>
   </children>
</VBox>
